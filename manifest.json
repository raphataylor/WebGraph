{
    "manifest_version": 2,
    "name": "WebGraph",
    "version": "1.0",
    "description": "Visualize your bookmarks",
    "permissions": [
      "bookmarks",
      "storage"
    ],
    "browser_action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
  }