{
    "name": "webgraph",
    "version": "1.0.0",
    "private": true,
    "dependencies": {
        "d3": "^7.0.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-scripts": "^5.0.1"
    },
    "devDependencies": {
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "build-extension": "INLINE_RUNTIME_CHUNK=false react-scripts build && npm run post-build",
        "post-build": "cp manifest.json build/ && cp src/background.js build/ && cp public/*.png build/ && cp public/favicon.ico build/"
      },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
